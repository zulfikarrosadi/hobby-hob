							"const collectionUserId = pm.collectionVariables.get('userId')\r",
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"JSON Body is valid\", function () {\r",
							"    var { user } = pm.response.json();\r",
